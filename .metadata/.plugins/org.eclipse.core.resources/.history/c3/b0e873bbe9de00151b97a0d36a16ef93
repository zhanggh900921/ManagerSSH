function drawLocation(x,y,z) {
	
	
	var location=new Array();
	   for(var j=0;j<=20;j++) {
	   		location[j]=new Array();
	   }
	   

	   location[7][1][0]=-200;
	   location[7][1][1]=-150;
	   location[7][2][0]=200;
	   location[7][2][1]=-150;
	   location[7][3][0]=0;
	   location[7][3][1]=0;
	   location[7][4][0]=0;
	   location[7][4][1]=100;
	   location[7][5][0]=-200;
	   location[7][5][1]=250;
	   location[7][6][0]=200;
	   location[7][6][1]=250;
	   location[7][7][0]=0;
	   location[7][7][1]=-250;

	   
	   
	   return location[x][y];
	
}