																							 		LS.removeItem('txtuser');
																							 		LS.removeItem('txtPwd');
																							 	}
																							 }
                                                                                         	 p=/^[\d]+$/;
																				
																							 	$("#user_login").hide();//登录框隐藏
																									$("#login_button").hide();//按钮隐藏
