                                                                                    errorMsg.push('地址不合法');
                                                                                }
                                                                                else 
                                                                                    if (res == "mac_error") {
                                                                                        errorMsg.push('MAC地址不合法');
                                                                                    }
                                                                                    else 
                                                                                        if (res == "sync_error") {
                                                                                            errorMsg.push('用户的资料被修改，正在同步');
                                                                                        }

                                                                                        else {
                                                                                             arr=res.split(",");
                                                                                             if(arr[0]>0){
																							 		LS.removeItem('Uid');
                                                                                           
																							 if (LS.isSupportLocalStorage) {
																							 	if ($("select:[name='save_me']").val() == '1') {
																							 		LS.item('txtuser', $("input[name='uname']").val());
