																									$("#login_ok_date").show(500);//提示成功
                                                                                             }
																							 else{
																							 	errorMsg.push(res);
																							 }
                                                                                            // return false;
                                                                                        }
                            ShowErrorMessage(errorMsg);
                        }
                    });
                }
                return false;
            }
            
            function ValidateLoginDetails(){
                var errorMsg = [];
                var user = $('#txtuser').val();
                var pwd = $('#txtPwd').val();
                var type = '1';
                var msg_user = '';
                var msg_pwd = '';
                if (type == '2') {
                    msg_user = '手机号码';
                    msg_pwd = '检验码';
                }
                else {
                    msg_user = '用户名';
                    msg_pwd = '密码';
                }
                if (user == '' || user == '用户名*' || user == '手机号码*') {
                    errorMsg.push('请输入' + msg_user + '.');
                    $('#txtuser').focus();
                }
                if (pwd == '' || pwd == '密码*' || pwd == '检验码*') {
                    errorMsg.push('请输入' + msg_pwd + '.');
                    $('#txtPwd').focus();
                }
                if (errorMsg != "") {
                    ShowErrorMessage(errorMsg);
                    return false;
                }
                return true;
            }
            
            function ShowErrorMessage(errors_){
                if (errors_.length > 0) {
                    $('#divloginError').html('').html($('<ul/>', {
                        html: '<li>' + errors_.join('</li><li>') + '</li>'
                    })).show();
                    $('#dvloginErrorM').show();
                    $.mobile.silentScroll(125);
                }
                else {
                    $('#dvloginErrorM').hide();
                }
            }
				function BindEnter(obj){
    var button = $('.click_enter');
